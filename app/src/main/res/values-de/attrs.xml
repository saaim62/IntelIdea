<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConnectavoFontAttributes">
        <attr name="fontType" format="enum">
            <enum name="Regular" value="0" />
            <enum name="Bold" value="1" />
            <enum name="SemiBold" value="2" />
            <enum name="Light" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="RoundedCornerImageView">
        <attr name="radius" format="dimension"/>
        <attr name="corner">
            <flag name="topLeft" value="1"/>
            <flag name="topRight" value="2"/>
            <flag name="bottomRight" value="3"/>
            <flag name="bottomLeft" value="4"/>
            <flag name="left" value="5"/>
            <flag name="right" value="6"/>
            <flag name="top" value="7"/>
            <flag name="bottom" value="8"/>
        </attr>
    </declare-styleable>
</resources>